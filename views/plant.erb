<h1>Explore the Plant World Around You!</h1>

<br>
<p>See all of the <u><%= @plant[:plant_title] %></u> in your area below.</p>
</br>


<% for @plant_location in @plant_instances %>
<br>
<ul class="list-unstyled">
  <li class="media">
    <i class="fas fa-leaf"></i>
    <div class="media-body">
      <h5 class="mt-0 mb-1">


        <%= @plant_location[:latitude] %>, <%= @plant_location[:longitude] %>
      
      </h5>
    </div>
</ul>
</br>
<% end %>  

<p> <a href="/plants/<%= @plant[:plant_id] %>/uniqueplant/new" class="btn btn-primary btn-lg active" style="background-color: #000080; role="button" aria-pressed="true"> Add your own <%= @plant[:plant_title] %></a></p>
